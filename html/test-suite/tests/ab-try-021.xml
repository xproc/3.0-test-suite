<t:test xmlns:t="http://xproc.org/ns/testsuite/3.0"
        xmlns:err="http://www.w3.org/ns/xproc-error"
        expected="fail" code="err:XS0112">
   <t:info>
      <t:title>AB try 021</t:title>
      <t:revision-history>
         <t:revision>
            <t:date>2019-07-13</t:date>
            <t:author>
               <t:name>Achim Berndzen</t:name>
            </t:author>
            <t:description xmlns="http://www.w3.org/1999/xhtml">
               <p>New test</p>
            </t:description>
         </t:revision>
      </t:revision-history>
   </t:info>
   <t:description xmlns="http://www.w3.org/1999/xhtml">
      <p>Test XS0112 is raised if p:finally has primary output port.</p>
   </t:description>
   <t:pipeline>
      <p:declare-step name="pipeline" xmlns:p="http://www.w3.org/ns/xproc" version="3.0">
         <p:output port="result" />
         <p:try>
            <p:identity>
               <p:with-input><initial /></p:with-input>
            </p:identity>
            <p:catch>
               <p:identity>
                  <p:with-input><catch /></p:with-input>
               </p:identity>
            </p:catch>
            <p:finally>
               <p:output port="finally" />
               <p:identity>
                  <p:with-input><finally /></p:with-input>
               </p:identity>
            </p:finally>
         </p:try>         
      </p:declare-step>
   </t:pipeline>
</t:test>