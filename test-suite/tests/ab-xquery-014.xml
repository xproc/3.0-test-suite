<t:test xmlns:t="http://xproc.org/ns/testsuite/3.0"
      xmlns:err="http://www.w3.org/ns/xproc-error"
      expected="fail" code="err:XC0009">
   <t:info>
      <t:title>p:xquery 014 (AB)</t:title>
      <t:revision-history>
         <t:revision>
            <t:date>2019-09-13</t:date>
            <t:author>
               <t:name>Achim Berndzen</t:name>
            </t:author>
            <t:description xmlns="http://www.w3.org/1999/xhtml">
               <p>Tests for p:xquery (Partly ported from 1.0)</p>
            </t:description>
         </t:revision>
      </t:revision-history>
   </t:info>
   <t:description xmlns="http://www.w3.org/1999/xhtml">
      <p>Tests p:xquery: Value for @version is not valid, so XC0009 is expected.</p>
   </t:description>
   
   <t:pipeline>
      <p:declare-step xmlns:p="http://www.w3.org/ns/xproc" version="3.0">
         <p:output port="result"/>
         
         <p:xquery version="Use any version you like.">
            <p:with-input port="source">
               <doc>
                  <para>A para.</para>
               </doc>
            </p:with-input>
            <p:with-input port="query">
               <result>//para</result>
            </p:with-input>
         </p:xquery>
         <p:wrap-sequence wrapper="result" />
      </p:declare-step>
   </t:pipeline>
</t:test>